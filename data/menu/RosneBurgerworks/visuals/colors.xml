    <Box padding="12 6 6 6" width="content" height="content" name="Checkbox" x="163" y="270">
        <List width="150">
            <AutoVariable width="fill" target="zk.style.checkbox.color.border" label="Border"/>
            <AutoVariable width="fill" target="zk.style.checkbox.color.checked" label="Checked"/>
            <AutoVariable width="fill" target="zk.style.checkbox.color.hover" label="Hover"/>
        </List>
    </Box>
    <Box padding="12 6 6 6" width="content" height="content" name="Other" x="163" y="340">
        <List width="150">
            <AutoVariable width="fill" target="colors.guicolor" label="GUI color"/>
